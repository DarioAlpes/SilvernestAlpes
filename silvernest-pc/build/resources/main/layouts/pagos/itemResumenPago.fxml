<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<fx:root xmlns="http://javafx.com/javafx/8.0.112"
         xmlns:fx="http://javafx.com/fxml/1"
         type="javafx.scene.layout.GridPane"
         styleClass="raiz"
         alignment="CENTER">
    <columnConstraints>
        <ColumnConstraints percentWidth="55"/>
        <ColumnConstraints percentWidth="45"/>
    </columnConstraints>

    <Label fx:id="labelNombreDelProducto"
           styleClass="etiqueta-14px-blanca"
           wrapText="true"
           text="# transacción"
           GridPane.columnIndex="0" GridPane.rowIndex="0"/>

    <Label fx:id="labelNombresReglasAplicadas"
           styleClass="etiqueta-14px-blanca"
           wrapText="true"
           text="# transacción"
           GridPane.columnIndex="1" GridPane.rowIndex="0">
        <GridPane.margin>
            <Insets left="20"/>
        </GridPane.margin>
    </Label>

    <stylesheets>
        <URL value="@/css/estilo.css"/>
    </stylesheets>

</fx:root>
